                <li class="friendsearchbox">
                    <form class="navbar-form" id="searchfriends" role="search" method="get" action="">
                        <div class="input-group">
                            <input  class="form-control search-friend" placeholder="Search Friends" name="searchfrnd" id="search-box-frnd">
                            <div class="input-group-btn">
                                <button class="btn btn-default" id="searchfrndbtn" type="button" ><i class="glyphicon glyphicon-search"></i></button>
                            </div>
                        </div>
                    </form>
                </li>
               <li id="header_inbox_bar" class="dropdown" ng-init="getNotifications()">
                  <a data-toggle="dropdown" class="dropdown-toggle" href="#" title="Notifications">
                  <i class="fa fa-bell-o" data-original-title="" title=""></i>
                  <span class="badge bg-important" data-ng-bind="notificationresults.length"></span>
                  </a>
                  <ul class="dropdown-menu extended inbox">
                     <div class="notify-arrow notify-arrow-red"></div>
                     <li>
                        <p class="red" >You have {{notificationresults.length}} new notifications</p>
                     </li>
                     <li data-ng-if="notificationresults.length > 0"  ng-repeat="notification in notificationresults" id="li_{{ notification._id}}">
                        <span class="closeButton" ng-click="removeNotification(notification._id)"><i class="fa fa-times" aria-hidden="true"></i></span>
                        <a href="javascript:void(0)">
                            <span class="photo">
                                <img alt="avatar" src="{{ notification.profileImage }}">

                            </span>
<!--                            <span class="subject">
                                <span class="from">{{ notification.notificationBy }}</span>
                                <span class="time" ng-bind="notification.addedOn | date:'yyyy-MM-dd'"></span>
                            </span>-->
                            <span class="message" data-ng-if="notification.notificationType == 'request'"><b>{{ notification.FirstName }}&nbsp;{{ notification.LastName }}</b> sent you a friend request.</span>
                            <span class="message" data-ng-if="notification.notificationType == 'invitation'"><b>{{ notification.FirstName }}&nbsp;{{ notification.LastName }}</b> sent you an event invitation.</span>
                            <span class="message" data-ng-if="notification.notificationType == 'follow'"><b>{{ notification.FirstName }}&nbsp;{{ notification.LastName }}</b> started following you.</span>
                            <span class="message" data-ng-if="notification.notificationType == 'acceptrequest'"><b>{{ notification.FirstName }}&nbsp;{{ notification.LastName }}</b> accepted your friend request.</span>
                            <span class="message" data-ng-if="notification.notificationType == 'eventjoined'"><b>{{ notification.FirstName }}&nbsp;{{ notification.LastName }}</b> accepted your event invitation.</span>

                            <span class="subject">
                                <!--<span class="time" ng-bind="notification.addedOn | date:'yyyy-MM-dd'"></span>-->
                                <!--<span class="time" ng-init="showtime(notification.addedOn)">{{ stopwatch }}</span>-->

                            </span>
                        </a>

                     </li>
