 <div id="header">
      <div class="overlay">
         <div class="container">
            <ul class="nav navbar-nav navbar-right top-menu">
                <li class="friendsearchbox"> 
                    <form class="navbar-form" id="searchfriends" role="search" method="get" action="/searchFriends">
                        <div class="input-group">
                            <input  class="form-control search-friend" placeholder="Search Friends" name="searchfrnd" id="search-box-frnd">
                            <div class="input-group-btn">
                                <button class="btn btn-default" id="searchfrndbtn" type="submit" ><i class="glyphicon glyphicon-search"></i></button>
                            </div>
                        </div>
                    </form>
                </li>
               <li id="header_inbox_bar" class="dropdown">
                  <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                  <i class="fa fa-envelope-o" data-original-title="" title=""></i>
                  <span class="badge bg-important">5</span>
                  </a>
                  <ul class="dropdown-menu extended inbox">
                     <div class="notify-arrow notify-arrow-red"></div>
                     <li>
                        <p class="red">You have 5 new messages</p>
                     </li>
                     <li>
                        <a href="#">
                            <span class="photo">
                                <img alt="avatar" src="<%= user.local.profileImage %>">
                            </span>
                            <span class="subject">
                                <span class="from"><%= user.local.firstName+' '+user.local.lastName %></span>
                                <span class="time">Just now</span>
                            </span>
                            <span class="message">Hello, this is an example msg.</span>
                        </a>
                     </li>
                     <li>
                        <a href="#">
                        <span class="photo"><img alt="avatar" src="http://bootemplates.com/themes/srikandi/v2/img/avatar-mini.jpg"></span>
                        <span class="subject">
                        <span class="from"><%= user.local.firstName+' '+user.local.lastName %></span>
                        <span class="time">Just now</span>
                        </span>
                        <span class="message">
                        Hello, this is an example msg.
                        </span>
                        </a>
                     </li>
                     <li>
                        <a href="#">See all messages</a>
                     </li>
                  </ul>
               </li>
               <!-- user login dropdown start-->
               <li class="dropdown">
                  <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                  <img style="width: 30px;" class="img-circle" src="<%= user.local.profileImage %>">
                  <span class="username"><%= user.local.firstName+' '+user.local.lastName %></span>
                  <b class="caret"></b>
                  </a>
                  <ul class="dropdown-menu">
                     <div class="log-arrow-up"></div>
                     <% if (user.local.userLevel == "ADMIN") {  %>
                     <li><a href="/admin"><i class="fa fa-user" data-original-title="" title=""></i> Admin Dashboard</a></li>
                     <% } %>
                     <li><a href="/profile"><i class="fa fa-user" data-original-title="" title=""></i> Profile</a></li>
                     <li><a href="/update-profile"><i class=" fa fa-suitcase" data-original-title="" title="Update Profile"></i>Update Profile</a></li>
                     <li><a href="/list-events"><i class="fa fa-user" data-original-title="" title=""></i>Events</a></li>
                     <li><a href="/friends"><i class="fa fa-user" data-original-title="" title=""></i>Friends</a></li>
                     <li><a href="/logout"><i class="fa fa-key" data-original-title="" title=""></i> Log Out</a></li>
                  </ul>
               </li>
            </ul>
         </div>
         <nav class="navbar" role="navigation">
            <div class="container">
               <!-- Brand and toggle get grouped for better mobile display -->
               <div class="navbar-header">
                  <button type="button" class="btn-block btn-drop navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                  <strong>MENU</strong>
                  </button>
               </div>
            </div>
         </nav>
      </div>
    </div>
   