            <!-- Modal Header -->
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal">
                    <span aria-hidden="true">&times;</span>
                    <span class="sr-only">Close</span>
                </button>
                <h4 class="modal-title" id="myModalLabel">Update Your Profile Image</h4>
            </div>
            <!-- Modal Body -->
            <form class="form-horizontal" id="uploadForm" action="/profileimage" enctype="multipart/form-data"
                  method="post">
