<div class="col-md-3">
  <div class="box box-primary">
    <div class="box-body box-profile">
      <img class="profile-user-img img-responsive img-circle" src="<%= result.local.profileImage %>" alt="User profile picture">
      <h3 class="profile-username text-center"> <%= result.local.firstName +' '+ result.local.lastName %> </h3>
      <p class="text-muted text-center"><%= result.local.locationState %>, <%= result.local.locationCountry %></p>
      <ul class="list-group list-group-unbordered social-icons-admin">
        <li class="list-group-item">
          <b>Followers</b> <a class="pull-right">0</a>
        </li>
        <li class="list-group-item">
          <b>Following</b> <a class="pull-right">0</a>
        </li>
        <li class="list-group-item">
          <b>Friends</b> <a class="pull-right">0</a>
        </li>
        <li class="list-group-item">
            <a href="<%= result.facebookURL %>" class=""><i class="fa fa-facebook-square"></i></a>
            <a href="<%= result.twitterURL %>" class=""><i class="fa fa-twitter-square"></i></a>
            <a href="<%= result.youtubeURL %>" class=""><i class="fa fa-youtube-square"></i></a>
            <a href="<%= result.skypeUsername %>" class=""><i class="fa fa-skype"></i></a>
            <a href="<%= result.instagramURL %>" class=""><i class="fa fa-instagram"></i></a>
        </li>
      </ul>
<!--      <a href="#" class="btn btn-primary btn-block"><b>Follow</b></a>-->
    </div>
  </div>
