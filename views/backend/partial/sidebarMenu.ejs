<aside class="main-sidebar">
    <section class="sidebar">
      <div class="user-panel">
        <div class="pull-left image">
            <% if (user !== undefined && user !== null) {  %>
                <img src="<%= user.local.profileImage %>" class="img-circle" alt="User Image">
            <% } %>
        </div>
        <div class="pull-left info">
          <p>
            <% if (user !== undefined && user !== null) {  %>
                <%= user.local.firstName+' '+user.local.lastName %>
            <% } %>
          </p>
          <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
        </div>
      </div>
      <ul class="sidebar-menu">
        <li class="treeview <% if(active == "list-users" || active == "view-user"){ %> active <% } %>">
          <a href="#">
            <i class="fa fa-users"></i> <span> Users Management</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li <% if(active == "list-users" || active == "view-user"){ %> class="active" <% } %>><a href="/admin/list-users"><i class="fa fa-list"></i> List Users</a></li>
          </ul>
        </li>

        <li class="treeview <% if(active == "add-category" || active == "list-category"){ %> active <% } %>">
        <a href="#">
